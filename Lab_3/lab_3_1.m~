clearvars;
clc;
close all; % Clearing workspace of matlab
 %Program
load funkcjeLUT %Loading the matlab object
lut_operations = {kwadratowa, log, odwlog, odwrotna, pierwiastkowa, pila, wykladnicza};
%Ploting an egzemplary function
%figure(1);
%plot(pila);
 %Reading data from files
 lena_color = imread('lena.bmp');
lena_gray = imread('lena_gray.bmp');
 circle = imread('kolo.bmp');
square = imread('kwadrat.bmp');
jet = imread('jet.bmp');
 img_data_list = {lena_color, lena_gray, circle, square, jet};
 figure(1)
LUT(lena_color, kwadratowa)
 figure(2)
LUT(jet, odwrotna)
 figure(3)
LUT(jet, odwlog)
 figure(4)
LUT(jet, pila)
 figure(5)
LUT(jet, wykladnicza)
 figure(6)
LUT(jet, log)